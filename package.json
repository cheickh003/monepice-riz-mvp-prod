    "postbuild": "npm run sentry:sourcemaps"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@sentry/nextjs": "^8.45.0",
    "appwrite": "^16.0.4",
    "lucide-react": "^0.533.0",
    "next": "15.4.4",
    "node-appwrite": "^14.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@sentry/cli": "^2.45.1",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.4.7",
    "eslint": "^9",
